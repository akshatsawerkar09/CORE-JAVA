package nested_classes;

public class Outer {

	private int i;
	private static int j;

	static {
		j = 100;
	}

	public Outer(int i) {
		super();
		this.i = i;
	}

	// static method
	public static void show() {
		System.out.println("Outer's show : static " + j);
	}

	// non static method
	public void display() {
		System.out.println("Outer's display : non static " + i + " static " + j);
	}

}
