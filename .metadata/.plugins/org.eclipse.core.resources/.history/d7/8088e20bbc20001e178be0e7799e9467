package p1;

class A {
	public static void main(String[] args) {
		int data;
		System.out.println(data); // data : method local var and javac doesn't allow accessing any un-init var
		System.out.println("In A's main");
	}
}

public class B {
	public static void main(String[] args) {
		System.out.println("In B's main");
	}
}

class C {
	public static void main(String[] args) {
		System.out.println("In C's main");
	}
}
