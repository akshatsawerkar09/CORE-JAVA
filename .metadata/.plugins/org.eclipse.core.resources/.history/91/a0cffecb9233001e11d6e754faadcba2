package p2;

import java.util.Arrays;

public class Test1 {

	public static void main(String[] args) {

		int[] data = { 12, 34, 1, 67, 89, 44, 567 };

		// objective : display ints : FP

		// array -> sequential stream -> forEach
		Arrays.stream(data).forEach(i -> System.out.print(i + " "));
	}

}
